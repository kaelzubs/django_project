RewriteEngine On

RewriteCond %{HTTP_HOST} ^www\.epredictz\.com$
RewriteRule ^/?$ "http\:\/\/epredictz\.com\/" [R=301,L]

<IfModule mod_expires.c>
    ExpiresActive on

    ExpiresByType text/css/main.css "access plus 1 month"
 </IfModule>

 <IfModule mod_expires.c>
