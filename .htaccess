    ExpiresByType img/aguero_.jpg "access plus 1 month"
    ExpiresByType img/messi.jpg "access plus 1 month"
    ExpiresByType img/neymar_.jpg "access plus 1 month"
    ExpiresByType img/Cr7c.jpg "access plus 1 month"
